direction: right

SV: "72-Dim State Vector" {
  style: {
    fill: "#f5f5f5"
    stroke: "#616161"
    border-radius: 8
    shadow: true
    bold: true
    font-size: 14
  }

  Base: "Inherited (12 dims)" {
    style: {
      fill: "#e3f2fd"
      stroke: "#1976d2"
      border-radius: 8
      shadow: true
      bold: true
      font-size: 13
    }
    SV1: "L1 Execution (4)\ngoal_alignment\nresponse_quality\nerror_count\ntoken_usage" {
      style: { fill: "#bbdefb"; stroke: "#1976d2"; border-radius: 6; font-size: 11 }
    }
    SV2: "L2 Strategy (4)\nstrategy_efficiency\nreasoning_diversity\nfailure_pattern\nplan_revision" {
      style: { fill: "#bbdefb"; stroke: "#1976d2"; border-radius: 6; font-size: 11 }
    }
    SV3: "L3 Identity (4)\nself_consistency\nidentity_stability\ngoal_persistence\nvalue_conflict" {
      style: { fill: "#bbdefb"; stroke: "#1976d2"; border-radius: 6; font-size: 11 }
    }
  }

  MSCP: "MSCP Additions (42 dims)" {
    style: {
      fill: "#c8e6c9"
      stroke: "#2e7d32"
      border-radius: 8
      shadow: true
      bold: true
      font-size: 13
    }
    SV4: "v1 (6): prediction_error,\nhash, agency, alignment..." {
      style: { fill: "#a5d6a7"; stroke: "#2e7d32"; border-radius: 6; font-size: 11 }
    }
    SV5: "v1.3 (6): consecutive_updates,\ncumulative_delta, cooldown..." {
      style: { fill: "#a5d6a7"; stroke: "#2e7d32"; border-radius: 6; font-size: 11 }
    }
    SV6: "v2 (8): mutation_count,\nvalue_lock, meta_depth..." {
      style: { fill: "#a5d6a7"; stroke: "#2e7d32"; border-radius: 6; font-size: 11 }
    }
    SV7: "v3 (9): belief_entropy,\nvelocity, ethical_score..." {
      style: { fill: "#a5d6a7"; stroke: "#2e7d32"; border-radius: 6; font-size: 11 }
    }
    SV8: "v3.1 (11): composite_stability,\nconvergence, budget..." {
      style: { fill: "#a5d6a7"; stroke: "#2e7d32"; border-radius: 6; font-size: 11 }
    }
  }

  V4: "v4 Additions (18 dims)" {
    style: {
      fill: "#e1bee7"
      stroke: "#8e24aa"
      border-radius: 8
      shadow: true
      bold: true
      font-size: 13
    }
    SV9: "Affect (9): 5 emotions,\ndominant, valence,\narousal, drives" {
      style: { fill: "#ce93d8"; stroke: "#8e24aa"; border-radius: 6; font-size: 11 }
    }
    SV10: "Survival (7): threat_level,\nintensity, goal_count,\nhomeostatic_deviation..." {
      style: { fill: "#ce93d8"; stroke: "#8e24aa"; border-radius: 6; font-size: 11 }
    }
    SV11: "Meta (2): timestamp,\nloop_iteration" {
      style: { fill: "#ce93d8"; stroke: "#8e24aa"; border-radius: 6; font-size: 11 }
    }
  }

  # Evolution flow: dimensions accumulate across versions
  Base -> MSCP: "extends" { style.animated: true; style.stroke: "#2e7d32" }
  MSCP -> V4: "extends" { style.animated: true; style.stroke: "#8e24aa" }

  # Internal version progression
  Base.SV1 -> Base.SV2 -> Base.SV3 { style.animated: true; style.stroke: "#1976d2" }
  MSCP.SV4 -> MSCP.SV5 -> MSCP.SV6 -> MSCP.SV7 -> MSCP.SV8 { style.animated: true; style.stroke: "#2e7d32" }
  MSCP.SV8 -> V4.SV9: "v4" { style.animated: true; style.stroke: "#8e24aa" }
  V4.SV9 -> V4.SV10 -> V4.SV11 { style.animated: true; style.stroke: "#8e24aa" }
}
