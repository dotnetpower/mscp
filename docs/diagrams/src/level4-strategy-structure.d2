StratLib: "Strategy Library" {
  style: {
    fill: "#e3f2fd"
    stroke: "#1976d2"
    border-radius: 8
    shadow: true
  }

  S1: "Strategy v1.0\n(active)" {
    style: {
      fill: "#bbdefb"
      stroke: "#1976d2"
      border-radius: 6
    }
  }

  S2: "Strategy v0.9\n(archived)" {
    style: {
      fill: "#bbdefb"
      stroke: "#1976d2"
      border-radius: 6
    }
  }

  S3: "Strategy v0.8\n(archived)" {
    style: {
      fill: "#bbdefb"
      stroke: "#1976d2"
      border-radius: 6
    }
  }

  S1 -> S2: "parent" {
    style.stroke-dash: 5
  }
  S2 -> S3: "parent" {
    style.stroke-dash: 5
  }
}

Params: "Strategy Parameters" {
  style: {
    fill: "#fff9c4"
    stroke: "#f9a825"
    border-radius: 8
    shadow: true
  }

  PP1: "exploration_rate in [0,1]" {
    style: {
      fill: "#fff59d"
      stroke: "#f9a825"
      border-radius: 6
    }
  }

  PP2: "risk_tolerance in [0,1]" {
    style: {
      fill: "#fff59d"
      stroke: "#f9a825"
      border-radius: 6
    }
  }

  PP3: "planning_depth in {1..5}" {
    style: {
      fill: "#fff59d"
      stroke: "#f9a825"
      border-radius: 6
    }
  }

  PP4: "goal_flexibility in [0,1]" {
    style: {
      fill: "#fff59d"
      stroke: "#f9a825"
      border-radius: 6
    }
  }

  PP5: "learning_aggressiveness in [0,1]" {
    style: {
      fill: "#fff59d"
      stroke: "#f9a825"
      border-radius: 6
    }
  }
}

Score: "Strategy Score" {
  style: {
    fill: "#e8f5e9"
    stroke: "#388e3c"
    border-radius: 8
    shadow: true
  }

  SS: "StrategyScore(s,t) =" {
    style: {
      fill: "#c8e6c9"
      stroke: "#388e3c"
      border-radius: 6
    }
  }

  SS1: "E_LTV(s,t)  Long-term reward" {
    style: {
      fill: "#c8e6c9"
      stroke: "#388e3c"
      border-radius: 6
    }
  }

  SS2: "- 0.3 * SI(s,t)  Stability impact" {
    style: {
      fill: "#c8e6c9"
      stroke: "#388e3c"
      border-radius: 6
    }
  }

  SS3: "- 0.2 * RC(s,t)  Resource cost" {
    style: {
      fill: "#c8e6c9"
      stroke: "#388e3c"
      border-radius: 6
    }
  }

  SS4: "- 0.2 * RF(s,t)  Risk factor" {
    style: {
      fill: "#c8e6c9"
      stroke: "#388e3c"
      border-radius: 6
    }
  }
}

StratLib -> Params: {
  style.animated: true
}

Params -> Score: {
  style.animated: true
}
